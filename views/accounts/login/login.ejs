<div class="container">
  <form id="submit-signup" action="/auth/login" method="POST">
    <!-- email -->
    <div class="form-group">
      <label for="InputEmail">Email address</label>
      <input id="authemail" type="email" class="form-control" name="email" placeholder="Enter email">
    </div>
    <!-- password -->
    <div class="form-group">
      <label for="InputPassword">Password</label>
      <input id="authpassword" type="password" class="form-control" name="password" placeholder="Password">
    </div>

    <button type="submit" class="btn btn-primary">Log In</button>

  </form>
</div>

<div class="signUp">
  <br>
  <a href="/signup">
  <button class="btn btn-primary">Sign Up</button>
  </a>
</div>
